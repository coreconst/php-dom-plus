{
    "name": "coreconst/php-dom-plus",
    "description": "This library extends DOMDocument and adds methods similar to those found in native JavaScript.",
    "type": "library",
    "keywords": [
        "php",
        "scraper",
        "parser",
        "web",
        "dom",
        "document"
    ],
    "autoload": {
        "psr-4": {
            "PhpDomPlus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpDomPlus\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Kostya Parfylo",
            "email": "parfilokostya0@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "guzzlehttp/guzzle": "^7.9",
        "ext-dom": "*",
        "symfony/css-selector": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0.1"
    }
}
